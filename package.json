{
  "name": "react-mad-navbar",
  "private": true,
  "version": "0.1.0",
  "description": "a navbar",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf example_dist/*",
    "copy": "copyfiles -f ./example/index.html ./example/favicon.ico ./example_dist",
    "dist:example": "npm run copy & webpack --env=example_dist",
    "dist": "webpack --env=dist",
    "lint": "eslint ./src",
    "posttest": "npm run lint",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:patch": "npm version patch && npm publish && git push --follow-tags",
    "serve": "node server.js --env=dev",
    "serve:example": "node server.js --env=example_dist",
    "start": "node server.js --env=dev"
  },
  "repository": "https://github.com/madwiki/react-mad-navbar.git",
  "keywords": ["react","navbar","navigation"],
  "author": "madwiki",
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.0.0",
    "babel-plugin-transform-proto-to-assign": "^6.6.5",
    "babel-plugin-transform-object-assign": "^6.5.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "bower-webpack-plugin": "^0.1.9",
    "chai": "^3.2.0",
    "copyfiles": "^0.2.1",
    "css-loader": "^0.23.0",
    "eslint": "~2.2.0",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-react": "^4.0.0",
    "file-loader": "^0.8.4",
    "glob": "^7.0.0",
    "isparta-instrumenter-loader": "^1.0.0",
    "minimist": "^1.2.0",
    "node-sass": "^3.4.2",
    "null-loader": "^0.1.1",
    "open": "0.0.5",
    "postcss": "^5.0.11",
    "postcss-loader": "^0.8.0",
    "react-addons-test-utils": "^0.14.0",
    "react-hot-loader": "^1.2.9",
    "rimraf": "^2.4.3",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.13.0",
    "webpack-dev-server": "1.10.1",
    "assign-deep": "^0.4.5",
    "react-redux": "^4.4.1",
    "react-router-redux": "^4.0.0",
    "redux": "^3.3.1",
    "redux-thunk": "^2.0.1"
  },
  "dependencies": {
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-router": "^2.0.1",
    "react-svg-morph": "0.1.3"
  },
  "license": "MIT"
}
